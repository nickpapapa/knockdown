/* pages/sequence/sequence.wxss */
page{
  min-height:100%;
}
.container{
  display: flex;
  justify-content: start;
  flex-direction: column;
  padding: 0 0 140rpx 0;
}

.title{
  margin: 20rpx;
  color: antiquewhite;
  text-align: center;

}

.question{
  color: antiquewhite;
  margin: 40rpx 20rpx;
  font-size: 30rpx;
  line-height: 45rpx;
}

.options {
  margin-bottom: 150rpx;
}
.option {
  min-width: 500rpx;
  border-radius: 30rpx;
  border: solid white 8rpx;
  margin: 30rpx;
  text-align: left;
  font-size: 24rpx;
}

.option-selected {
  min-width: 500rpx;
  border-radius: 30rpx;
  border: solid #e4ae1a 8rpx;
  margin: 30rpx;
  text-align: left;
  font-size: 24rpx;
}


.icon{
  padding: 26rpx;
  width: 48rpx;
  height: 48rpx;
}
.favorate{
  position: relative;
  border-radius: 50rpx;
  
  background-color: whitesmoke;
  background-repeat: false;
}

.footer{
  box-sizing: border-box;
  border-top: 2rpx solid gray;
  background-color: #2E4A62; 
  color: #DAF7A6;
  position: fixed;
  bottom: 0;
  width: 750rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  display: flex;
  flex-direction: row;
  align-content: center;
}

.btn{
  background-color: #2E4A62; 
  border: 1rpx solid #2E4A62;
  color: #F0EDE5;
  width: 300rpx;
  height: 100rpx;
  line-height: 100rpx;
}

/* 去除button的边框 */
.btn::after{
  border: none;
}

.page {
  height: 30rpx;
  padding: 0 0 0 37rpx;
  font-size: 24rpx;
  color: #999;
  background: url("data:image/png;base64,") no-repeat left center/28rpx 28rpx;
  line-height: 36rpx;
}

.status-lists {
  max-height: 500rpx;
  padding: 0 0 24rpx;
  border-top: 1rpx solid #eaeaea;
  background: #fff;
  z-index: 100;
  overflow: auto;
}

.status-lists .status-list {
  float: left;
  width: 98rpx;
  height: 98rpx;
  margin: 24rpx 0 0 20rpx;
  border-radius: 50rpx;
  border: 1rpx solid #ccc;
  font-size: 20rpx;
  line-height: 98rpx;
  text-align: center;
  color: #ccc;
}



